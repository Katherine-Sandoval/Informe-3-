# Informe-3-
String myText= "" ;


void setup ( ) {
  size(300, 300);
} 


void draw () { 
  background (0, 0, 0);
  rect( 150, 150, 50, 50);
  if (mousePressed && (mouseButton == LEFT)) { 
    fill(0, 255, 0);
    text(myText, 100, 100); 
    myText= "ON" ;
  }
  
  if (mousePressed && (mouseButton == RIGHT)) {
    fill(255,0,0); 
    text(myText, 100, 100);
    myText= "OFF" ;
  }
}
